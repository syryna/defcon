
<!-- container for map buttons -->


<div id="map_buttons" class="btn-toolbar mb-3" role="toolbar" aria-label="Toolbar with button groups">
    <div class="btn-group" role="group" aria-label="First group">
        <!-- control the view -->
        <button type="button" class="btn btn-outline-warning btn-sm" id="zoom_reset" data-toggle="tooltip" data-placement="bottom" 
            title="Reset Position and Zoom"><span class="fas fa-expand fa-fw" aria-hidden="true"></span></button>
        <button type="button" class="btn btn-outline-warning btn-sm" id="center" data-toggle="tooltip" data-placement="bottom" 
            title="Center the Map"><span class="fas fa-compress-arrows-alt fa-fw" aria-hidden="true"></span></button>
        <button type="button" class="btn btn-outline-warning btn-sm" id="zoom_in" data-toggle="tooltip" data-placement="bottom" 
            title="Zoom In"><span class="fas fa-plus fa-fw" aria-hidden="true"></span></button>
        <button type="button" class="btn btn-outline-warning btn-sm" id="zoom_out" data-toggle="tooltip" data-placement="bottom" 
            title="Zoom Out"><span class="fas fa-minus fa-fw" aria-hidden="true"></span></button>
    </div>
    <div class="btn-group" role="group" aria-label="Second group">
        <!-- search -->
        <button type="button" class="btn btn-outline-warning btn-sm" id="search_loc" data-toggle="modal" data-target="#search_modal"  data-placement="bottom" 
            title="Search for System"><span class="fas fa-search-location fa-fw" aria-hidden="true"></span></button>
    </div>
    <div class="btn-group" role="group" aria-label="Third group">
        <!-- switch UI -->
        <% if (flavor == 'mobile') { %>
            <a class="btn btn-outline-warning btn-sm" href="/dashboard/region?id=<%= query %>" role="button" data-toggle="tooltip" data-placement="bottom" title="Return to Desktop Version"><span class="fas fa-desktop fa-fw" aria-hidden="true"></span></a>
        <% } else {%> 
            <a class="btn btn-outline-warning btn-sm" href="/dashboard/region?id=<%= query %>&flavor=mobile" role="button" data-toggle="tooltip" data-placement="bottom" title="Open Clean Window for Mobiles"><span class="fas fa-tablet-alt fa-fw" aria-hidden="true"></span></a>
        <%}%> 
    </div>
</div>

<div id="user_buttons" class="btn-toolbar mb-3" role="toolbar" aria-label="Toolbar with button groups">
    <div class="btn-group" role="group" aria-label="First group">
        <!-- user bar -->
        <button type="button" class="btn btn-outline-danger btn-sm" id="toggle_legend" data-toggle="tooltip" data-placement="bottom" 
            title="Turn ON/OFF legend"><span class="fas fa-question fa-fw" aria-hidden="true"></span></button>
        <button type="button" class="btn btn-outline-success btn-sm" id="toggle_labels" data-toggle="tooltip" data-placement="bottom" 
            title="Turn ON/OFF labels"><span class="fas fa-tag fa-fw" aria-hidden="true"></span></button>
        <button type="button" class="btn btn-outline-danger btn-sm" id="toggle_message_box" data-toggle="tooltip" data-placement="bottom" 
            title="Turn ON/OFF Alert Box"><span class="far fa-envelope fa-fw" aria-hidden="true"></span></button> 
        <button type="button" class="btn btn-outline-success btn-sm" id="toggle_tooltips" data-toggle="tooltip" data-placement="bottom" 
            title="Turn ON/OFF System Tooltips"><span class="fas fa-comment-alt fa-fw" aria-hidden="true"></span></button> 
        <button type="button" class="btn btn-outline-success btn-sm" id="toggle_trails" data-toggle="tooltip" data-placement="bottom" 
            title="Turn ON/OFF Alert Trails"><span class="fas fa-share-alt fa-fw" aria-hidden="true"></span></button> 
        <button type="button" class="btn btn-outline-warning btn-sm" id="save_map_settings" data-toggle="tooltip" data-placement="bottom" 
            title="Save current position and on/off settings for region"><span class="far fa-save fa-fw" aria-hidden="true"></span></button>
    </div>
    <div class="btn-group" role="group" aria-label="Second group">
        <!-- toggle button -->
        <a id="user_settings" class="btn btn-outline-warning btn-sm" href="#" role="button" data-toggle="tooltip" data-placement="bottom" title="Change User Settings"><span class="fas fa-user-cog fa-fw" aria-hidden="true"></span></a>
        <!-- <a id="array_dump" class="btn btn-outline-warning btn-sm" href="#" role="button" data-toggle="tooltip" data-placement="bottom" title="Show message array"><span class="fas fa-layer-group fa-fw" aria-hidden="true"></span></a> -->
    </div>
</div>

